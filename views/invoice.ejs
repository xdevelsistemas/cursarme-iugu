<!--{   unidade : req.params.unidade ,-->
<!--codmovimento : req.params.movimento,-->
<!--parcela : req.params.parcela-->
<!--}-->
<% if (unidade && codmovimento && parcela ) { %>

<div id="g1" ng-controller="regeraBoletoController">
    <div class="ui one item menu">
        <a class="active item">
            <i class="clock icon"></i> Boleto expirado
        </a>
    </div>
    <div class="ui segment page grid">
        <div class="eight centered column">
            <% if (aviso) { %>
            <div class="ui negative message">
                <div class="header">
                    Opa, seu boleto expirou
                </div>
                <p>Clique no bot√£o abaixo para regerar com a data de vencimento do dia.
                </p>
            </div>
            <%}%>
            <div class="ui negative message" ng-show="erro">
                <i class="ui icon close"></i>
                <div class="header">
                    Problemas ao regerar o boleto
                </div>
                <p>{{erro}}
                </p>
            </div>
            <form class="ui form segment">
                <div class="sixteen centered column" ng-click="click(<%= unidade %>, <%= codmovimento %> ,<%= parcela %>)">
                    <div class="fluid ui submit button negative ">Regerar</div>
                </div>
            </form>
            <a ng-show="valid" target="_blank" href="{{boleto}}" class="fluid ui submit button positive">Visualizar o Boleto</a>
        </div>
    </div>
</div>

<% } %>

